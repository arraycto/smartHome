<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Mq2Servlet</servlet-name>
        <servlet-class>com.hr.mqtt.web.Mq2Servlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>Mq2Servlet</servlet-name>
        <url-pattern>/mq2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TempHumiServlet</servlet-name>
        <servlet-class>com.hr.mqtt.web.TempHumiServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>TempHumiServlet</servlet-name>
        <url-pattern>/temphumi</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LightServlet</servlet-name>
        <servlet-class>com.hr.mqtt.web.LightServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LightServlet</servlet-name>
        <url-pattern>/light</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>QueryTempHumiServlet</servlet-name>
        <servlet-class>com.hr.mqtt.web.QueryTempHumiServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>QueryTempHumiServlet</servlet-name>
        <url-pattern>/th</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>QueryMq2Servlet</servlet-name>
        <servlet-class>com.hr.mqtt.web.QueryMq2Servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>QueryMq2Servlet</servlet-name>
        <url-pattern>/mq</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>QueryLightServlet</servlet-name>
        <servlet-class>com.hr.mqtt.web.QueryLightServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>QueryLightServlet</servlet-name>
        <url-pattern>/li</url-pattern>
    </servlet-mapping>

</web-app>